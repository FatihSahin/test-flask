<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata minClientVersion="2.8">
    <id>TestFlask.Assistant</id>
    <version>$version$</version>
    <title>TestFlask.Assistant</title>
    <authors>Fatih Sahin</authors>
    <owners>Fatih Sahin</owners>
    <developmentDependency>true</developmentDependency>
    <licenseUrl>http://www.opensource.org/licenses/mit-license.php</licenseUrl>
    <projectUrl>http://github.com/FatihSahin/test-flask</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <!--<iconUrl></iconUrl>-->
    <description>TestFlask.Assistant</description>
    <summary>
      This package contains test flask assistant ASP.NET MVC controllers, and scripts to enable test flask easy recording plugin inside your existing ASP.NET MVC application
    </summary>
    <language>en-US</language>
    <tags>TestFlask</tags>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Configuration"  />
      <frameworkAssembly assemblyName="System.Net.Http"  />
      <frameworkAssembly assemblyName="System.Web" />
      <frameworkAssembly assemblyName="System.ServiceModel" />
      <frameworkAssembly assemblyName="System.ServiceModel.Web" />
    </frameworkAssemblies>
    <dependencies>
      <dependency id="Microsoft.AspNet.WebApi.Client" version="5.2.3" />
      <dependency id="Newtonsoft.Json" version="6.0.4" />
    </dependencies>
  </metadata>
  <files>
    <file src="bin\$Configuration$\TestFlask.Models.dll" target="lib\net452"  />
    <file src="bin\$Configuration$\MongoDB.Bson.dll" target="lib\net452"  />
    <file src="*.xdt" target="content" />
    <file src="Areas\TestFlask\**\*.*" target="content\Areas\TestFlask" />
    <file src="Scripts\testFlask.js" target="content\Scripts" />
    <file src="Content\testFlask.css" target="content\Content" />
  </files>
</package>
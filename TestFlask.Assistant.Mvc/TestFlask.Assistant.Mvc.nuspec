<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata minClientVersion="2.8">
    <id>TestFlask.Assistant.Mvc</id>
    <version>$version$</version>
    <title>TestFlask.Assistant.Mvc</title>
    <authors>Fatih Sahin</authors>
    <owners>Fatih Sahin</owners>
    <developmentDependency>true</developmentDependency>
    <licenseUrl>http://www.opensource.org/licenses/mit-license.php</licenseUrl>
    <projectUrl>http://github.com/FatihSahin/test-flask</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <!-- Erlenmeyer Flask by Rockicon from the Noun Project -->
    <iconUrl>https://raw.githubusercontent.com/FatihSahin/test-flask/master/Icons/package_icon_green.png</iconUrl>
    <description>TestFlask.Assistant.Mvc</description>
    <summary>This package contains TestFlask Assistant ASP.NET MVC controllers, WCF behaviors, Web API client message handlers specific to work inside MVC, scripts to ease plugging TestFlask capabilities into your existing ASP.NET MVC application.</summary>
    <language>en-US</language>
    <tags>TestFlask, Testing, ASP.NET MVC, Fody, Cecil, Weaver</tags>
    <dependencies>
      <dependency id="TestFlask.Assistant.Core" version="1.0.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="bin\$Configuration$\TestFlask.Models.dll" target="lib\net452"  />
    <file src="bin\$Configuration$\MongoDB.Bson.dll" target="lib\net452"  />
    <file src="*.xdt" target="content" />
    <file src="Areas\TestFlask\**\*.*" target="content\Areas\TestFlask" />
    <file src="Scripts\testFlask.js" target="content\Scripts" />
    <file src="Content\testFlask.css" target="content\Content" />
  </files>
</package>